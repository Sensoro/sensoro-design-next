{
  "name": "@sensoro-design/plots",
  "version": "2.0.0",
  "description": "Sensoro Design G2 Plots.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sensoro/sensoro-design-next.git",
    "directory": "@sensoro-design/plots"
  },
  "sideEffects": false,
  "main": "src/index.ts",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib es dist",
    "build": "pnpm clean && pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc --target ES5 --module commonjs --outDir lib",
    "build:esm": "tsc --target ES5 --module ESNext --outDir es"
  },
  "peerDependencies": {
    "lodash-es": "^4.17.21",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@ant-design/plots": "^2.2.8",
    "@antv/g": "^6.0.0",
    "@antv/g2": "^5.1.18",
    "@antv/util": "^3.3.5",
    "@rcuse/shared": "^0.25.0"
  },
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.js",
    "module": "./es/index.js",
    "types": "./es/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./es/index.d.ts",
          "default": "./es/index.js"
        },
        "require": {
          "types": "./lib/index.d.ts",
          "default": "./lib/index.js"
        }
      },
      "./es/*": "./es/*",
      "./lib/*": "./lib/*",
      "./package.json": "./package.json"
    }
  }
}
