{
  "name": "@sensoro-design/charts",
  "version": "2.0.0",
  "description": "Sensoro Design Chart Library.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sensoro/sensoro-design-next.git",
    "directory": "@sensoro-design/charts"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "main": "src/index.ts",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib es dist",
    "build": "pnpm clean && pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc --target ES5 --module commonjs --outDir lib",
    "build:esm": "tsc --target ES5 --module ESNext --outDir es"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@sensoro-design/react": "workspace:*",
    "clsx": "^2.1.1"
  },
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.js",
    "module": "./es/index.js",
    "types": "./es/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./es/index.d.ts",
          "default": "./es/index.js"
        },
        "require": {
          "types": "./lib/index.d.ts",
          "default": "./lib/index.js"
        }
      },
      "./es/*": "./es/*",
      "./lib/*": "./lib/*",
      "./package.json": "./package.json"
    }
  },
  "authors": [
    "zhangfuhao@sensoro.com"
  ]
}
