{
  "name": "@sensoro-design/react",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "walrus build-react"
  },
  "keywords": [
    "react",
    "react-components",
    "components",
    "library",
    "design-system",
    "sensoro",
    "sensoro-design",
    "ui"
  ],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./es/index.d.ts",
        "default": "./es/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "files": [
    "es",
    "lib",
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lotusjs/lotus-design.git",
    "directory": "packages/lotus-design"
  },
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@lotus-design/react-primitives": "workspace:^",
    "@pansy/classnames": "^1.0.1",
    "@rcuse/core": "^0.19.0",
    "dayjs": "^1.11.11"
  },
  "publishConfig": {
    "access": "public"
  }
}
