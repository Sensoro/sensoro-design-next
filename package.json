{
  "name": "@sensoro-design/monorepo",
  "version": "2.0.0",
  "private": true,
  "packageManager": "pnpm@9.4.0",
  "license": "MIT",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "website": "pnpm --filter website dev",
    "dev": "pnpm storybook",
    "build": "walrus build",
    "build:all": "walrus build all",
    "build:docs": "pnpm build:storybook",
    "build:react": "pnpm --filter=@sensoro-design/react run build",
    "example:vite": "pnpm --filter=@example/vite run start",
    "release": "walrus release",
    "release:alpha:patch": "walrus release patch --stage alpha --tag next",
    "release:alpha:minor": "walrus release minor --stage alpha --tag next",
    "release:alpha:major": "walrus release major --stage alpha --tag next",
    "release:patch": "npm run release patch",
    "release:minor": "npm run release minor",
    "release:major": "npm run release major",
    "storybook": "pnpm --filter @sensoro-design/storybook start",
    "build:storybook": "pnpm --filter @sensoro-design/storybook build",
    "start:storybook": "pnpm --filter @sensoro-design/storybook start",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "prepare": "husky || true",
    "clean:node-modules": "rm -rf node_modules && rm -rf packages/*/node_modules",
    "clean:lock": "rm -rf pnpm-lock.yaml && rm -rf package.lock.json",
    "clean:dist": "rm -rf packages/*/es && rm -rf packages/*/lib",
    "clean:all": "pnpm clean:lock && pnpm clean:node-modules && pnpm clean:dist",
    "reinstall": "pnpm clean:all && npm run bootstrap",
    "preinstall": "npx only-allow pnpm",
    "major": "taze minor -wIr"
  },
  "dependencies": {
    "@ant-design/plots": "^2.2.8",
    "@antv/util": "^3.3.8",
    "@floating-ui/react": "^0.26.22",
    "@lotus-design/react-primitives": "^1.0.0-alpha.3",
    "@rcuse/core": "^0.25.0",
    "@sensoro-design/styles": "1.27.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "react-number-format": "^5.4.0",
    "react-remove-scroll": "^2.5.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.21.2",
    "@antfu/ni": "^0.21.12",
    "@antv/g-svg": "^2.0.9",
    "@antv/g2": "^5.2.2",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint-react/eslint-plugin": "^1.5.15",
    "@lowcode/monaco-editor": "^1.0.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/d3-interpolate": "^3.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/signale": "^1.4.7",
    "@types/yargs": "^17.0.32",
    "@vitest/ui": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "cssnano": "^7.0.5",
    "cypress": "^13.13.2",
    "cypress-real-events": "^1.13.0",
    "cypress-vite": "^1.5.0",
    "d3-interpolate": "^3.0.1",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "execa": "^9.3.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.4",
    "jsdom": "^24.1.1",
    "lerna": "^8.1.6",
    "less": "^4.2.0",
    "lint-staged": "^15.2.8",
    "new-github-release-url": "^2.0.0",
    "ora": "^8.0.1",
    "pnpm": "^9.4.0",
    "postcss": "^8.4.41",
    "postcss-less": "^6.0.0",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.20.0",
    "rollup-plugin-banner2": "^1.3.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "signale": "^1.4.0",
    "simple-git": "^3.25.0",
    "stylelint": "^16.8.1",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "taze": "^0.16.3",
    "tsx": "^4.17.0",
    "type-fest": "^4.24.0",
    "typescript": "^5.4.5",
    "version-next": "^1.0.2",
    "vite": "^5.4.0",
    "vite-plugin-style-import": "^2.0.0",
    "vitest": "^2.0.5",
    "walrus": "workspace:*",
    "yargs": "^17.7.2",
    "zx": "^8.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix"
    ]
  }
}
